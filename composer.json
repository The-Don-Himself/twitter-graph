{
    "name": "the-don-himself/twitter-graph",
    "description": "An example graph database implementation of Twitter using the Gremlin OGM library.",
    "type": "library",
    "require": {
        "php": "^7.2",
        "abraham/twitteroauth": "^1.0",
        "symfony/console": "^5.0 || ^4.0 || ^3.0",
        "symfony/yaml": "^5.0 || ^4.0 || ^3.0",
        "the-don-himself/gremlin-ogm": "^0.5"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Don Omondi",
            "email": "don.e.omondi@gmail.com"
        }
    ],
    "keywords": [
        "Twitter",
        "graph",
        "Amazon Neptune",
        "Azure Cosmos DB",
        "Datastax Enterprise Graph",
        "JanusGraph",
        "Neo4j",
        "Neo4j",
        "OrientDB",
        "gremlin-php",
        "gremlin-ogm"
    ],
    "autoload": {
        "psr-4": {
            "TheDonHimself\\TwitterGraph\\": "src/"
        }
    },
    "minimum-stability": "stable",
    "bin": ["bin/graph", "bin/graph.php"]
}
